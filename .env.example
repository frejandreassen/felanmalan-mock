# Google Maps API Key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API Mode
# Set to "true" to use mock API (development)
# Set to "false" to use real FAST2 API (production)
NEXT_PUBLIC_MOCK_API=true

# FAST2 API Configuration (only needed when NEXT_PUBLIC_MOCK_API=false)
FAST2_BASE_URL=https://klient-test.fabo.se:8243
CONSUMER_KEY=your_consumer_key_here
CONSUMER_SECRET=your_consumer_secret_here

# FAST2 User Credentials (for future user-based authentication)
USERNAME=your_username_here
PASSWORD=your_password_here
